@model CIP.Web.Models.Especialidad.EspecialidadDetailViewModel

@{
    ViewBag.Title = "Especialidades";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Detalle de la Especialidad</h1>
        <ol class="breadcrumb">
            <li><i class="fa fa-dashboard"></i> <a href="@Url.Action("Index", "Especialidad")">Especialidad</a></li>
            <li><i class="fa fa-list"></i> <a href="@Url.Action("Index", "Postulante")">Postulante</a></li>
            <li class="active"><i class="fa fa-edit"></i></li> Detalle de Especialidad
        </ol>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel-heading text-box main-text">
        Detalle de Especialidad
    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Nombre</th>
                        <th>Descripcion</th>
                        @*<th>Fecha</th>
                        <th>Telefono</th>*@
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.EspecialidadesDetail.Nombre</td>
                        <td>@Model.EspecialidadesDetail.Descripcion</td>
                        @*<td>@Model.PostulantesDetail.Fecha</td>
                        <td>@Model.PostulantesDetail.Telefono</td>*@


                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

