@model CIP.Web.Models.Postulante.PostulanteDetailViewModel

@{
    ViewBag.Title = "Postulantes";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Detalle de Postulante</h1>
        <ol class="breadcrumb">
            <li><i class="fa fa-dashboard"></i> <a href="@Url.Action("Index", "Home")">Dashboard</a></li>
            <li><i class="fa fa-list"></i> <a href="@Url.Action("Index", "Postulante")">Postulante</a></li>
            <li class="active"><i class="fa fa-edit"></i></li> Detalle de Postulante
        </ol>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel-heading text-box main-text">
        Detalle de Postulante
    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Apellidos y Nombres</th>
                        <th>DNI</th>
                        <th>Fecha</th>
                        <th>Telefono</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.PostulantesDetail.DatosPostulante</td>
                        <td>@Model.PostulantesDetail.DNI</td>
                        <td>@Model.PostulantesDetail.Fecha</td>
                        <td>@Model.PostulantesDetail.Telefono</td>


                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel-heading text-box main-text">
        Especialidad a la que Postula
    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Especialidad</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.PostulantesDetail.Especialidades.Nombre</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
